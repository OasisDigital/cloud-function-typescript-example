    "@types/node": "^7.0.8",
    "chai": "^3.5.0",
    "copyfiles": "^1.2.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
